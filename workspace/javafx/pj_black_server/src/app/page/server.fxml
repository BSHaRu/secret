<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.Server">
   <bottom>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="exit" contentDisplay="CENTER" mnemonicParsing="false" text="Stop" />
         </children>
      </VBox>
   </bottom>
   <center>
      <VBox alignment="CENTER" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="log" editable="false" maxHeight="300.0" maxWidth="300.0" minHeight="300.0" minWidth="300.0" prefHeight="300.0" prefWidth="300.0" promptText="Log" />
            <HBox maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
               <children>
                  <TextField fx:id="msg" maxHeight="26.0" maxWidth="240.0" minHeight="26.0" minWidth="240.0" prefHeight="26.0" prefWidth="240.0" />
                  <Button fx:id="sSend" maxWidth="60.0" minWidth="60.0" mnemonicParsing="false" prefWidth="60.0" text="전송" />
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets top="40.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <padding>
      <Insets top="20.0" />
   </padding>
</BorderPane>
